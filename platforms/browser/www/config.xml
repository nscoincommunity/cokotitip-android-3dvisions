<?xml version='1.0' encoding='utf-8'?>
<widget id="com.tigadsaja.adamcell" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/3.1.0">
    <name>Adam Cell</name>
    <description>
        Aplikasi PPOB
    </description>
    <author email="cokotitip@gmail.com" href="https://cokotitip.com">
        Cokotitip Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="6" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
